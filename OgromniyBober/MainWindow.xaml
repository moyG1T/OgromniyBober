<Window x:Class="OgromniyBober.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:OgromniyBober"
        mc:Ignorable="d"
        Title="MainWindow" Height="720" Width="960">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="1*"/>
            <RowDefinition Height="7*"/>
        </Grid.RowDefinitions>
        <Rectangle Fill="#272727"></Rectangle>
        <TextBlock Name="MainTitle" Grid.Row="0" FontSize="40" VerticalAlignment="Center" HorizontalAlignment="Center">Название</TextBlock>

        <Frame x:Name="MainWindowFrame" Grid.Row="1" NavigationUIVisibility="Hidden"></Frame>
    </Grid>
</Window>
